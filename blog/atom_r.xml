<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Juuso's blog on Open Data Science and R</title>
 <link href="http://blog.mschwarz.us/blog/atom_r.xml" rel="self"/>
 <link href="http://blog.mschwarz.us/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://blog.mschwarz.us/blog</id>
 <author>
   <name>Michael Schwarz</name>
   <email>schwarz.mike@gmail.com</email>
 </author>
 
 {% for post in site.tags["R"] limit:10 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://blog.mschwarz.us{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>id:{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>